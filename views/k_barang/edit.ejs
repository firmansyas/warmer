<% include ../partials/header %>
<% include ../partials/menu %>
<% include ../partials/sidebar %>
<div class="bg-dark dk" id="wrap">
  <!-- #menu -->
  <div id="content">
    <div class="outer">
      <div class="inner bg-light lter">

        <div class="row">
          <div class="col-lg-12">
            <div class="box">
              <header>
                <div class="icons"><i class="fa fa-edit"></i></div>
                <h5>Edit Data Kategori Barang</h5>
              </header>

              <div id="div-4" class="body">
                <form class="form-horizontal" method="POST" action="/k_barang/edit/<%= idURL %>">

                  <div class="form-group">
                    <label for="text1" class="control-label col-lg-2">Kode Kategori Barang</label>
                    <div class="col-lg-9">
                      <input type="text" id="text1" placeholder="" name="kokbarang" class="form-control" value="<%= k_barangData.kokbarang %>">
                    </div>
                  </div>

                  <div class="form-group">
                    <label for="text1" class="control-label col-lg-2">Nama Kategori Barang</label>
                    <div class="col-lg-9">
                      <input type="text" id="text1" placeholder="" name="nakabar" class="form-control" value="<%= k_barangData.nakabar %>">
                    </div>
                  </div>

                  <div class="form-actions no-margin-bottom">
                    <label class="control-label col-lg-2"></label>
                    <input type="submit" value="Submit" class="btn btn-primary">
                    <a href="/k_barang" <button class="btn btn-danger" type="button" name="button">Cancel </a> </button>
                  </div>
                </div>

              </div>
            </form>
          </div>

        </div>
      </div>
    </div>

  </div>
</div>




<% include ../partials/footer %>
